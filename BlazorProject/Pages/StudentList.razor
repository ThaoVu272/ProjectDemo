@page "/"
@inherits StudentListBase

<h1>Call API</h1>
@if (getBranchesError)
{
    <p>Unable to get branches from GitHub. Please try again later.</p>
}
else
{
    <ul>
        @foreach (var student in Students)
        {
            <li>@student.StudentId</li>
            <li>@student.Name</li>
            <li>@student.Roll</li>
        }
    </ul>
}

